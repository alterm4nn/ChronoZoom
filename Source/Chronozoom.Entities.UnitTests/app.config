<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=5.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  <connectionStrings>
    <add name="Storage" connectionString="Data Source=(LocalDB)\v11.0;AttachDbFilename=|DataDirectory|\Storage.mdf;Integrated Security=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="BaseDataMigrationDirectory" value="..\..\..\Chronozoom.UI\" />
    <add key="MaxTimelinesToImport" value="10" />
  </appSettings>
  <entityFramework>
    <contexts>
      <context type="Chronozoom.Entities.Storage,Chronozoom.Entities">
        <databaseInitializer type="System.Data.Entity.MigrateDatabaseToLatestVersion`2[[Chronozoom.Entities.Storage,Chronozoom.Entities],
                              [Chronozoom.Entities.Test.StorageMigrationsConfiguration,Chronozoom.Entities.UnitTests]], EntityFramework"/>
      </context>
    </contexts>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
      <parameters>
        <parameter value="v11.0" />
      </parameters>
    </defaultConnectionFactory>
  </entityFramework>
</configuration>